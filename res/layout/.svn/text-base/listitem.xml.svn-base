<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:paddingTop="6.0px"
	android:paddingLeft="10.0px"
	android:paddingBottom="8.0px"
	android:layout_width="fill_parent" 
	android:layout_height="35.0px" 
	android:background="@drawable/list_item_normal">
<TextView 
    android:id="@+id/ListTextView" 
	android:layout_width="190.0px" 
	android:layout_height="fill_parent"
	android:textStyle="bold"
	android:textColor="#000000"/>
	<TextView 
    android:id="@+id/ListSubTextView" 
	android:layout_width="110.0px" 
	android:layout_height="fill_parent"
	android:textColor="#000000"/>
<ImageView
	android:id="@+id/ListImageView"
	android:layout_width="62.0px"
	android:layout_height="fill_parent"
	/>
	</LinearLayout>
	